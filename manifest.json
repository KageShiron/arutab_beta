{
  "manifest_version": 2,
  "name": "arutab",
  "version": "0",

  "description": "",
  
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-3.1.1.min.js",
        "js/content.js",
        "js/html2canvas.min.js"
      ],
      "css":["./content.css"]
    }
  ],
  "permissions": [
    "<all_urls>","storage","tabs"
  ],
  "background" : {
      "scripts":["js/background.js"]
  },
  "web_accessible_resources" : [ "tablist.html" ]
  
  }